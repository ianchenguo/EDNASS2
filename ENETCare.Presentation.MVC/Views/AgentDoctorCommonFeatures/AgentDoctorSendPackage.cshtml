
@{
    ViewBag.Title = "AgentDoctorSendPackage";
    Layout = "~/Views/AgentDoctorCommonFeatures/MasterPage.cshtml";
}

<!--panel-start-->
<div class="panel panel-default">
    <!--panel-heading-start-->
    <div class="panel-heading">
        <h3 class="panel-title">Send Medication Package</h3>
    </div>
    <!--panel-heading-end-->
    <!--panel-body-start-->
    <div class="panel-body">
        <div class="form-horizontal col-sm-10 col-sm-offset-1">

            <!-- <div id="AgentDoctorSendPackageAlertWindowDiv" visible="false" class="alert alert-success" data-dismiss="alert">
                <label id="AgentDoctorSendPackageAlertWindowContentLabel"></label>
            </div> -->

            <div class="form-group">

                <label id="AgentDoctorSendingDropDownListLabel" class="col-xs-3">Destination: </label>
                <div class="col-xs-9">
                    <select id="AgentDoctorSendingDropDownList" class="form-control">
                        <option value="" disabled selected>Please select</option>
                        <option>Liverpool Office</option>
                        <option>Gelbe Office</option>
                        <option>Ultimo Office</option>
                    </select>
                </div>
            </div>

            <div class="form-group">
                <label id="AgentDoctorSendPackageTypebarcodeLabel" class="col-xs-3">Barcode: </label>
                <div class="col-xs-9">
                    <input id="AgentDoctorSendPackageTypebarcodeInput" class="form-control" />
                </div>
            </div>

            <!-- the following link should be optimised -->
            <a class="btn btn-danger btn-float-right btn-margin-left" type="a" href="#">Cancel</a>
            <a href="#" class="btn btn-success btn-float-right" id="AgentDoctorSendPackageTypeButton">Send</a>
        </div>
    </div>
    <!--panel-body-end-->
</div>
<!--panel-end-->