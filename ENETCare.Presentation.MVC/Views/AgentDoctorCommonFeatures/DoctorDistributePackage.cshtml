@using ENETCare.Business;
@*@model ICollection<MedicationType>*@


@{
    ViewBag.Title = "DoctorDistributePackage";
    Layout = "~/Views/AgentDoctorCommonFeatures/MasterPage.cshtml";
}

<!--panel-start-->
<div class="panel panel-default">
    <!--panel-heading-start-->
    <div class="panel-heading">
        <h3 class="panel-title">Distribute Medication Package</h3>
    </div>
    <!--panel-heading-end-->
    <!--panel-body-start-->
@using (Html.BeginForm()){
    <div class="panel-body">
        <div class="form-horizontal col-sm-10 col-sm-offset-1">

            <!-- <div id="DoctorDistributeAlertWindowDiv" visible="false" class="alert alert-success" data-dismiss="alert">
                <asp:Label ID="DoctorDistributeAlertWindowContentLabel" />
            </div> -->

            <div class="form-group">
                <div id="DoctorDistributePackageTypebarcode" cssclass="col-xs-3">Barcode: </div>

                <div class="col-xs-9">
                    @Html.TextBox("DoctorDistributePackageTypebarcode", null, new { @class = "form-control", @type = "text" })
                </div>
            </div>

            <a class="btn btn-danger btn-float-right btn-margin-left" type="a" href="@Url.Action("AgentDoctorHomePackages", "AgentDoctorCommonFeatures")">Cancel</a>
            <a href="#" class="btn btn-success btn-float-right" id="DoctorDistributeButton">Distribute</a>
        </div>
    </div>
}
    <!--panel-body-end-->
</div>
<!--panel-end-->

